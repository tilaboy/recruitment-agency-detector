{
    "model_type": "tf_lstm_multi",
    "model_name": "staffing_agency_detector",
    "model_path": "models/tf/lstm",
    "model_file": "models/tf/sa_detector.h5",

    "features_enabled": false,
    "dropout_rate": 0.5,
    "optimizer": "Adam",
    "learning_rate": 0.02,
    "num_epochs": 100,
    "batch_size": 128,
    "log_dir": "log",
    "max_sequence_length": 512,

    "lstm": {
        "hidden_size": 150,
        "nr_layers": 2
    },

    "cnn": {
        "nr_layers": 1,
        "filter_size": 32,
        "kernel_size": 3
    },

    "embedding": {
        "file": "../../embeddings//en-wiki-and-cv-data-till-2016.bin",
        "dimension": 150,
        "token_encoding": "max_embedding",
        "trainable": false
    },

    "datasets": {
        "train": "../recruitment_agency_data/train",
        "eval": "../recruitment_agency_data/eval",
        "test": {
            "unindentified": "../recruitment_agency_data/unidentified/",
            "annotated": "../recruitment_agency_data/annotated/",
            "eval": "../recruitment_agency_data/eval"
        }
    }

}
